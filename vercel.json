{
  "buildCommand": "chmod +x scripts/download-artifact.sh && scripts/download-artifact.sh",
  "outputDirectory": "admin",
  "installCommand": "echo 'No install needed'",
  "framework": null,
  "rewrites": [{ "source": "/(.*)", "destination": "/index.html" }],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        }
      ]
    }
  ]
}
